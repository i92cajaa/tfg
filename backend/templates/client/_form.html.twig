        <div class="col-auto">
            {% for goal in form.goals.vars.choices %}
                <label class="me-2"><input {% if goal.value in field_value(form.goals) %} checked {% endif %} type="checkbox" name="{{ field_name(form.goals) }}" value='{{ goal.value }}' class="me-1">{{ goal.label }}</label>
            {% endfor %}
            {% for error in field_errors(form.goals) %}
                <span class="text-danger">{{ error }}</span>
            {% endfor %}
        </div>

    </div>

    <div class="row d-flex justify-content-end mt-2">
        <div class="mr-2 text-end">
            <button class="btn btn-primary text-white waves-effect waves-float waves-light ">{{ button_label }}</button>
        </div>
    </div>

{{ form_end(form) }}

{% block javascripts %}
    <script src="{{ asset('assets/js/select2/select2.full.min.js') }}"></script>
    <script src="{{ asset('assets/js/select2/intltelinput.min.js') }}"></script>
{% endblock %}