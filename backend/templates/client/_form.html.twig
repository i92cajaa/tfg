                <option {% if client.announcement == 1%} selected {% endif %}value="1">2022 - I Call AOF</option>
                <option {% if client.announcement == 2%} selected {% endif %}value="2">2023 - I Call AOF</option>
                <option {% if client.announcement == 3%} selected {% endif %}value="3">2023 - II Call AOF</option>
            </select>
            {% for error in field_errors(form.announcement) %}
                <span class="text-danger">{{ error }}</span>
            {% endfor %}
        </div>

