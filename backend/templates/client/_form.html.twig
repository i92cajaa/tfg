{{ form_start(form, {'attr': {'id': 'form'}}) }}
    <div id="client" class="row gy-3">

        <input type="hidden" id="client_delete_image" name="client[delete_image]" autocomplete="off">


        <h2 class="row gap-2 align-items-center m-2 gap-3">
            {% if client.name is not same as '' %}
                <div class="pic-container pic-small pic-circle p-0">
                    <img class="pic" src="{% if client.logo != null %}{{ documentImage.DocumentImage(client.logo) }}{% else %}{{ asset('assets/images/no-image.png') }}{% endif %}" alt="">
                    {% if client.logo != null %}
                        <div class="pic-overlay">
                            <a><i class="fa fa-trash-o text-danger fs-2" aria-hidden="true"></i></a>
                        </div>
                    {% endif %}
                </div>
            {% endif %}
            Información principal
        </h2><hr>
        <div class="col-md-6">
            <div class="row gy-4">
                <div class="col-md-6">
                    <label class="form-label" for="{{ field_name(form.logo) }}">{{ field_label(form.logo) }} <span class="text-danger"></span></label>
                    <input type="file" class="form-control"
                           placeholder="{{ field_label(form.logo) }}" id="{{ field_name(form.logo) }}" name="{{ field_name(form.logo) }}"
                           value="{{ field_value(form.logo) }}" accept="image/*">
                </div>
                <div class="col-md-6">
                    <label class="form-label" for="{{ field_name(form.name) }}">{{ field_label(form.name) }} <span class="text-danger">*</span></label>
                    <input class="form-control" type="text" placeholder="{{ field_label(form.name) }}" id="{{ field_name(form.name) }}" name="{{ field_name(form.name) }}" value="{{ field_value(form.name) }}" required>
                    {% for error in field_errors(form.name) %}
                        <span class="text-danger">{{ error }}</span>
                    {% endfor %}
                </div>
                <div class="col-md-6">
                    <label class="form-label" for="{{ field_name(form.center) }}">{{ field_label(form.center) }} <span class="text-danger">*</span></label>
                    <select class="form-select" type="text" id="{{ field_name(form.center) }}" name="{{ field_name(form.center) }}" required>
                        <option disabled selected value="{{ null }}">Selecciona una opción</option>
                        {% for center in centers %}
                            <option {% if center.id == field_value(form.center) %}selected{% endif %} value="{{ center.id }}">{{ center.name }}</option>
                        {% endfor %}
                    </select>
                    {% for error in field_errors(form.center) %}
                        <span class="text-danger">{{ error }}</span>
                    {% endfor %}
                </div>
                <div class="col-md-6">
                    <label class="form-label" for="{{ field_name(form.speciality) }}">{{ field_label(form.speciality) }} <span class="text-danger">*</span></label>
                    <input class="form-control" type="text" placeholder="{{ field_label(form.speciality) }}" id="{{ field_name(form.speciality) }}" name="{{ field_name(form.speciality) }}" value="{{ field_value(form.speciality) }}" required>
                    {% for error in field_errors(form.speciality) %}
                        <span class="text-danger">{{ error }}</span>
                    {% endfor %}
                </div>
            </div>
        </div>

        <div class="col-md-6">
            <label class="form-label" for="{{ field_name(form.description) }}">{{ field_label(form.description) }} <span class="text-danger">*</span></label>
            <textarea rows="5" class="form-control" placeholder="{{ field_label(form.description) }}" id="{{ field_name(form.description) }}" name="{{ field_name(form.description) }}" required>{{ field_value(form.description) }}</textarea>
            {% for error in field_errors(form.description) %}
                <span class="text-danger">{{ error }}</span>
            {% endfor %}
        </div>
        <div class="col-sm-3">
            <label class="form-label" for="{{ field_name(form.socialName) }}">{{ field_label(form.socialName)}}</label>
            <input class="form-control" type="text" placeholder="{{ field_label(form.socialName) }}" id="{{ field_name(form.socialName) }}" name="{{ field_name(form.socialName) }}" value="{{ field_value(form.socialName) }}">
            {% for error in field_errors(form.socialName) %}
                <span class="text-danger">{{ error }}</span>
            {% endfor %}
        </div>
        <div class="col-sm-3">
            <label class="form-label" for="{{ field_name(form.cif) }}">{{ field_label(form.cif)}}</label>
            <input class="form-control" type="text" placeholder="{{ field_label(form.cif) }}" id="{{ field_name(form.cif) }}" name="{{ field_name(form.cif) }}" value="{{ field_value(form.cif) }}">
            {% for error in field_errors(form.cif) %}
                <span class="text-danger">{{ error }}</span>
            {% endfor %}
        </div>
        <div class="col-sm-3">
            <label class="form-label" for="{{ field_name(form.province) }}">{{ field_label(form.province)}}</label>
            <input class="form-control" type="text" placeholder="{{ field_label(form.province) }}" id="{{ field_name(form.province) }}" name="{{ field_name(form.province) }}" value="{{ field_value(form.province) }}">
