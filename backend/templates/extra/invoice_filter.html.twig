 <form id="filter_form" name="form-filter-users w-100">
        <fieldset class="">
            <div class="d-flex justify-content-start align-items-center mx-50 row pt-0 pb-2">
                <input type="hidden" class="form-control" name="{{ filterService.filterField('verification_ids') }}" value="" form="filter_form">

                {% if filterService.getFilterValue("entity") == constant('App\\Entity\\Client\\Client::ENTITY') %}
                    <div class="form-group-feedback form-group-feedback-right mt-2 col-md-12 mb-1">
                        <label for="Nombre">{{ (configuration.client_nomenclature ~'s')|trans }}</label>
                        <select name="{{ filterService.filterField('client') }}" class="select select2">
                            <option value="" >{{ 'Select an option'|trans }}</option>
                            {% for client in clients %}
                                <option value="{{ client.id }}" {% if client.id == filterService.getFilterValue("client") %}selected="selected"{% endif %}>{{ client.name }} {{ client.surnames }}</option>
                            {% else %}
                                <option selected="selected" disabled="disabled">{{ 'No results found'|trans }}</option>
                            {% endfor %}

                        </select>
                        <div class="form-control-feedback">

                        </div>
                    </div>
                {% endif %}

                {% if filterService.getFilterValue("entity") == constant('App\\Entity\\User\\User::ENTITY') %}
                    <div class="form-group-feedback form-group-feedback-right mt-2 col-md-12 mb-1">
                        <label for="Nombre">{{ (configuration.user_nomenclature ~'s')|trans }}</label>
                        <select name="{{ filterService.filterField('user') }}" class="select select2">
                            <option value="">{{ 'Select an option'|trans }}</option>
                            {% for user in users %}
                                <option value="{{ user.id }}" {% if user.id == filterService.getFilterValue("user") %}selected="selected"{% endif %}>{{ user.name }} {{ user.surnames }}</option>
                            {% else %}
                                <option selected="selected" disabled="disabled">{{ 'No results found'|trans }}</option>
                            {% endfor %}

                        </select>
                        <div class="form-control-feedback">

                        </div>
                    </div>
                {% endif %}


                <div class="form-group-feedback form-group-feedback-right col-md-12 mb-1">
                    <label for="info">{{'Info'|trans}}</label>
                    <input type="text" class="form-control" placeholder="{{'Name'|trans }}, {{ 'Invoice Number'|trans }}..." name="{{ filterService.filterField('info') }}" value="{{ filterService.getFilterValue("info") }}" form="filter_form">
                    <div class="form-control-feedback">

                    </div>
                </div>

                <div class="form-group-feedback form-group-feedback-right col-md-12 mb-1">
                    <label for="week_day">{{'Date Range'|trans}}</label>
                    <br>
                    <input type="text" class="form-control" id="dateRangeFilter" placeholder="Selecciona un rango de dÃ­as" name="{{ filterService.filterField('dateRange') }}" value="{{ filterService.getFilterValue("dateRange") }}" form="filter_form">
                    <div class="form-control-feedback">

                    </div>
                </div>

                <div class="form-group-feedback form-group-feedback-right col-md-12 mb-1">
                    <label for="iva">{{ 'VAT'|trans }}</label>
                    <input type="text" class="form-control" name="{{ filterService.filterField('iva') }}" value="{{ filterService.getFilterValue("iva") }}" form="filter_form">
                    <div class="form-control-feedback">

                    </div>
                </div>

                <input type="hidden" id="filter_entity" name="{{ filterService.filterField('entity') }}" value="{{ filterService.getFilterValue("entity") }}" form="filter_form">


                {{ filterService.filterFormField() | raw }}

                <div class="form-group-feedback form-group-feedback-right text-end col-12">
                    <button type="submit" class="btn add-new text-white btn-primary mt-2">{{ 'Apply Filter'|trans }}</button>
                </div>
            </div>
        </fieldset>
    </form>


